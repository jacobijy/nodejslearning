extends layout

block content
  h1= title
  p Welcome to #{title}
  p Wow: #{result}
  form(action='/input', method='post')
    |Name
    input(type='text', name='name')
    |Url
    input(type='text', name='url')
    |Alexa
    input(type='text', name='alexa')
    |Country
    input(type='text', name='country')
    input(type='submit', value='submit')
  table(border='1', width = '400')
    - for(var i=0; i<6; i++)
        tr
          - for(var j=0; j<6; j++)
            td script(src="")